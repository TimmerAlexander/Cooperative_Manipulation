<?xml version="1.0" ?>
<launch>
  <arg name="robot_ip" value="192.168.13.90" />
  <arg name="load_gripper" default="true" />

  <include file="$(find franka_control)/launch/franka_control.launch" >
    <arg name="robot_ip" value="$(arg robot_ip)" />
    <arg name="load_gripper" value="$(arg load_gripper)" />
  </include>

  <!-- Launch the franka_joint_torque_controller -->
  <rosparam command="load" file="$(find cooperative_manipulation_controllers)/config/franka_joint_torque_controller.yaml" />
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"  args="franka_joint_torque_controller"/>

</launch>
